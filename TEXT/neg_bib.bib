@article{glass1,
author = {Amir, Ariel and Oreg, Yuval and Imry, Yoseph}, 
title = {On relaxations and aging of various glasses},
volume = {109}, 
number = {6}, 
pages = {1850-1855}, 
year = {2012}, 
doi = {10.1073/pnas.1120147109}, 
abstract ={Slow relaxation occurs in many physical and biological systems. ÒCreepÓ is an example from everyday life. When stretching a rubber band, for example, the recovery to its equilibrium length is not, as one might think, exponential: The relaxation is slow, in many cases logarithmic, and can still be observed after many hours. The form of the relaxation also depends on the duration of the stretching, the Òwaiting time.Ó This ubiquitous phenomenon is called aging, and is abundant both in natural and technological applications. Here, we suggest a general mechanism for slow relaxations and aging, which predicts logarithmic relaxations, and a particular aging dependence on the waiting time. We demonstrate the generality of the approach by comparing our predictions to experimental data on a diverse range of physical phenomena, from conductance in granular metals to disordered insulators and dirty semiconductors, to the low temperature dielectric properties of glasses.}, 
URL = {http://www.pnas.org/content/109/6/1850.abstract}, 
eprint = {http://www.pnas.org/content/109/6/1850.full.pdf}, 
journal = {Proceedings of the National Academy of Sciences} 
}

